  "license": "GPL-3.0",
  "scripts": {
    "format": "npm run clang-format -- --glob=\"@(src|e2e|examples)/**/*.@(ts|js)\" -i",
    "build": "ng build --prod",
    "serve": "ng serve angular-infinite-scroll-docs",
    "serve:e2e": "ng serve angular-infinite-scroll-e2e --port 3000",
    "e2e": "ng e2e",
    "e2e:update": "npm run webdriver-manager update",
    "release:major": "npm run standard-version -- --release-as major && git push --follow-tags",
    "release:minor": "npm run standard-version -- --release-as minor && git push --follow-tags",
    "release:patch": "npm run standard-version && git push --follow-tags",
    "clang-format": "clang-format",
    "standard-version": "standard-version",
    "conventional-github-releaser": "conventional-github-releaser",
    "webdriver-manager": "webdriver-manager"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0-rc.0 || ^6.0.0",
    "@angular/core": "^6.0.0-rc.0 || ^6.0.0"
  },
